{
  "env": {
    "browser": true,
    "es2020": true,
    "node": true
  },
  "extends": [
    "plugin:vue/essential",
    "standard",
    "plugin:json/recommended"
  ],
  "parserOptions": {
    "ecmaVersion": 2020,
    "sourceType": "module"
  },
  "plugins": [
    "vue"
  ],
  "rules": {
  },
  "globals": {
    "db": "readonly",
    "ipcRenderer": "readonly",
    "__static": "readonly",
    "describe": "readonly",
    "it": "readonly"
  }
}
