{
  "env": {
    "browser": true,
    "es2020": true,
    "node": true
  },
  "extends": [
    "plugin:vue/essential",
    "standard",
    "plugin:json/recommended"
  ],
  "parserOptions": {
    "ecmaVersion": 2020,
    "sourceType": "module"
  },
  "plugins": [
    "vue",
    "jest-dom"
  ],
  "rules": {
  },
  "globals": {
    "__static": "readonly",
    "describe": "readonly",
    "it": "readonly",
    "jest": "readonly",
    "expect": "readonly",
    "log": "readonly"
  }
}
