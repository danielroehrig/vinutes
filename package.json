{
  "name": "vinutes",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service electron:serve",
    "build": "vue-cli-service electron:build",
    "test:unit": "env NODE_ENV=test vue-cli-service test:unit --ui=bdd tests/unit --recursive",
    "test:e2e": "env NODE_ENV=test vue-cli-service test:unit -ui=bdd tests/e2e --recursive",
    "lint-fix": "eslint 'src/**' --fix",
    "electron:generate-icons": "electron-icon-builder --input=./public/icon.png --output=build --flatten",
    "i18n:report": "vue-cli-service i18n:report --src './src/**/*.?(js|vue)' --locales './src/locales/**/*.json'",
    "lint": "eslint 'src/**'",
    "postinstall": "electron-builder install-app-deps",
    "postuninstall": "electron-builder install-app-deps",
    "test:components": "env NODE_ENV=test vue-cli-service test:unit --ui=bdd tests/components --recursive"
  },
  "main": "background.js",
  "dependencies": {
    "@mdi/font": "^5.3.45",
    "better-sqlite3": "^7.1.0",
    "bulma": "^0.8.2",
    "calendar": "^0.1.1",
    "electron-log": "^4.2.2",
    "fluent-ffmpeg": "^2.1.2",
    "mime": "^2.4.6",
    "moment": "^2.26.0",
    "sharp": "^0.25.4",
    "vue": "^2.6.11",
    "vue-i18n": "^8.17.3",
    "vue-router": "^3.2.0",
    "vuex": "^3.4.0"
  },
  "devDependencies": {
    "@intlify/vue-i18n-loader": "^1.0.0",
    "@vue/cli-plugin-e2e-cypress": "^4.3.1",
